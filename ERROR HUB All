--// UI setup
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "AimbotUI"
screenGui.ResetOnSpawn = false
screenGui.IgnoreGuiInset = true -- ‡∏™‡∏≥‡∏Ñ‡∏±‡∏ç‡πÉ‡∏´‡πâ‡∏ï‡∏£‡∏á‡∏Å‡∏•‡∏≤‡∏á‡∏à‡∏£‡∏¥‡∏á
screenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")

-- ‡∏Ç‡∏ô‡∏≤‡∏î‡∏ß‡∏á‡∏Å‡∏•‡∏°
local circleSize = 250

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Frame ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ß‡∏á‡∏Å‡∏•‡∏°
local circle = Instance.new("Frame")
circle.Name = "CenterCircle"
circle.Size = UDim2.fromOffset(circleSize, circleSize)
circle.AnchorPoint = Vector2.new(0.5, 0.5) -- ‡∏à‡∏∏‡∏î‡∏≠‡πâ‡∏≤‡∏á‡∏≠‡∏¥‡∏á‡∏Å‡∏•‡∏≤‡∏á
circle.Position = UDim2.fromScale(0.5, 0.5) -- ‡∏ï‡∏£‡∏á‡∏Å‡∏•‡∏≤‡∏á‡∏à‡∏≠
circle.BackgroundTransparency = 1 -- ‡πÉ‡∏™‡πÇ‡∏õ‡∏£‡πà‡∏á‡∏î‡πâ‡∏≤‡∏ô‡πÉ‡∏ô
circle.Visible = true
circle.Parent = screenGui

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÄ‡∏™‡πâ‡∏ô‡∏ß‡∏á‡∏ô‡∏≠‡∏Å (Stroke)
