--// ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏´‡∏•‡∏±‡∏Å
local radius = circleSize / 2
local currentTween = nil
local tweenDuration = 0.10 -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏ô‡∏∏‡πà‡∏°‡∏ô‡∏ß‡∏•‡∏Ç‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏•‡πá‡∏á
local updateInterval = 0.01
local accumulatedTime = 0
local lockEnabled = false -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏õ‡∏¥‡∏î‡∏•‡πá‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤
local teamCheckEnabled = true -- ‡πÄ‡∏õ‡∏¥‡∏î‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ó‡∏µ‡∏°‡πÇ‡∏î‡∏¢‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
local currentTarget = nil -- ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ó‡∏µ‡∏°
local function IsEnemy(player)
    if not teamCheckEnabled then
        return true -- ‡∏ñ‡πâ‡∏≤‡∏õ‡∏¥‡∏î‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ó‡∏µ‡∏° ‡πÉ‡∏´‡πâ‡∏ñ‡∏∑‡∏≠‡∏ß‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏®‡∏±‡∏ï‡∏£‡∏π
    end
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏Å‡∏°‡∏°‡∏µ‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏µ‡∏°‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
    if not game:GetService("Teams"):GetChildren() or #game:GetService("Teams"):GetChildren() == 0 then
        return true -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ó‡∏µ‡∏°‡πÉ‡∏ô‡πÄ‡∏Å‡∏° ‡πÉ‡∏´‡πâ‡∏ñ‡∏∑‡∏≠‡∏ß‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏®‡∏±‡∏ï‡∏£‡∏π
    end
    
    local localTeam = LocalPlayer.Team
    local targetTeam = player.Team
    
    -- ‡∏ñ‡πâ‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ó‡∏µ‡∏° ‡∏´‡∏£‡∏∑‡∏≠‡∏ó‡∏µ‡∏°‡πÅ‡∏ï‡∏Å‡∏ï‡πà‡∏≤‡∏á‡∏Å‡∏±‡∏ô = ‡πÄ‡∏õ‡πá‡∏ô‡∏®‡∏±‡∏ï‡∏£‡∏π
    if not localTeam or not targetTeam or localTeam ~= targetTeam then
        return true
    end
    
    return false -- ‡πÄ‡∏õ‡πá‡∏ô‡∏ó‡∏µ‡∏°‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô
end

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Å‡∏≥‡πÅ‡∏û‡∏á (Wall Check)
local function CanSeeTarget(targetHead)
    if not targetHead then return false end
    
    local cameraPos = Camera.CFrame.Position
    local targetPos = targetHead.Position
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏î‡πâ‡∏ß‡∏¢ Raycast ‡∏ß‡πà‡∏≤‡∏°‡∏µ‡∏≠‡∏∞‡πÑ‡∏£‡∏ö‡∏±‡∏á‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á‡∏Å‡∏•‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ö‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
    local raycastParams = RaycastParams.new()
    raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
    raycastParams.FilterDescendantsInstances = {LocalPlayer.Character, targetHead.Parent}
    raycastParams.IgnoreWater = true
    
    local raycastResult = workspace:Raycast(cameraPos, (targetPos - cameraPos), raycastParams)
    
    -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏≠‡∏∞‡πÑ‡∏£‡∏ö‡∏±‡∏á ‡∏´‡∏£‡∏∑‡∏≠‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á‡∏ô‡πâ‡∏≠‡∏¢‡∏°‡∏≤‡∏Å = ‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡πÄ‡∏´‡πá‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÑ‡∏î‡πâ
    if not raycastResult then
        return true
    end
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏™‡∏¥‡πà‡∏á‡∏ó‡∏µ‡πà‡∏ö‡∏±‡∏á‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏Å‡∏±‡∏ö‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏°‡∏≤‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡πÅ‡∏Ñ‡πà‡πÑ‡∏´‡∏ô
    local hitDistance = (raycastResult.Position - cameraPos).Magnitude
    local targetDistance = (targetPos - cameraPos).Magnitude
    
    -- ‡∏ñ‡πâ‡∏≤‡∏™‡∏¥‡πà‡∏á‡∏ó‡∏µ‡πà‡∏ö‡∏±‡∏á‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏°‡∏≤‡∏Å (‡πÄ‡∏ú‡∏∑‡πà‡∏≠ËØØÂ∑Æ‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢) ‡πÉ‡∏´‡πâ‡∏ñ‡∏∑‡∏≠‡∏ß‡πà‡∏≤‡πÄ‡∏´‡πá‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
    if math.abs(hitDistance - targetDistance) < 2 then
        return true
    end
    
    return false
end

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏¢‡∏±‡∏á‡∏°‡∏µ‡∏ä‡∏µ‡∏ß‡∏¥‡∏ï‡∏≠‡∏¢‡∏π‡πà
local function IsPlayerAlive(player)
    if not player or not player.Character then
        return false
    end
    
    local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
    if not humanoid then
        return false
    end
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤ Humanoid ‡∏¢‡∏±‡∏á‡∏°‡∏µ‡∏™‡∏∏‡∏Ç‡∏†‡∏≤‡∏û‡πÅ‡∏•‡∏∞‡∏¢‡∏±‡∏á‡∏°‡∏µ‡∏ä‡∏µ‡∏ß‡∏¥‡∏ï‡∏≠‡∏¢‡∏π‡πà
    if humanoid.Health <= 0 then
        return false
    end
    
    return true
end

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
local function UpdateTargetInfo(player)
    if player and player.Character then
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        local head = player.Character:FindFirstChild("Head")
        
        if humanoid and head then
            -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á
            local distance = (head.Position - Camera.CFrame.Position).Magnitude
            local roundedDistance = math.floor(distance)
            
            -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï UI
            targetName.Text = "‡∏ä‡∏∑‡πà‡∏≠: " .. player.Name
            targetHealth.Text = "‡πÄ‡∏•‡∏∑‡∏≠‡∏î: " .. math.floor(humanoid.Health) .. "/" .. math.floor(humanoid.MaxHealth)
            targetDistance.Text = "‡∏£‡∏∞‡∏¢‡∏∞‡∏´‡πà‡∏≤‡∏á: " .. roundedDistance .. " studs"
            
            -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡∏™‡∏∏‡∏Ç‡∏†‡∏≤‡∏û‡∏ï‡∏≤‡∏°‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡πÄ‡∏•‡∏∑‡∏≠‡∏î
            if humanoid.Health / humanoid.MaxHealth < 0.3 then
                targetHealth.TextColor3 = Color3.new(1, 0, 0) -- ‡∏™‡∏µ‡πÅ‡∏î‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏ô‡πâ‡∏≠‡∏¢
            elseif humanoid.Health / humanoid.MaxHealth < 0.6 then
                targetHealth.TextColor3 = Color3.new(1, 1, 0) -- ‡∏™‡∏µ‡πÄ‡∏´‡∏•‡∏∑‡∏≠‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏õ‡∏≤‡∏ô‡∏Å‡∏•‡∏≤‡∏á
            else
                targetHealth.TextColor3 = Color3.new(0, 1, 0) -- ‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏°‡∏≤‡∏Å
            end
            
            targetInfoFrame.Visible = true
            return
        end
    end
    
    -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
    targetName.Text = "‡∏ä‡∏∑‡πà‡∏≠: -"
    targetHealth.Text = "‡πÄ‡∏•‡∏∑‡∏≠‡∏î: -"
    targetDistance.Text = "‡∏£‡∏∞‡∏¢‡∏∞‡∏´‡πà‡∏≤‡∏á: -"
    targetHealth.TextColor3 = Color3.new(1, 1, 1)
    targetInfoFrame.Visible = false
end

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡πá‡∏≠‡∏Å‡∏´‡∏±‡∏ß
local function LockToHead(targetHead)
	if not targetHead then return end
	local camPos = Camera.CFrame.Position
	local targetPos = targetHead.Position

	local goalCFrame = CFrame.new(camPos, targetPos)

	if currentTween then
		currentTween:Cancel()
	end

	local tweenInfo = TweenInfo.new(tweenDuration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	currentTween = TweenService:Create(Camera, tweenInfo, {CFrame = goalCFrame})
	currentTween:Play()
end

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏ô‡∏ß‡∏á‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î
local function GetClosestTarget()
	local screenCenter = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
	local closestPlayer = nil
	local shortestDistance = radius

	for _, player in pairs(Players:GetPlayers()) do
		if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("Head") then
			-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ó‡∏µ‡∏°‡∏Å‡πà‡∏≠‡∏ô
			if IsEnemy(player) then
				-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏¢‡∏±‡∏á‡∏°‡∏µ‡∏ä‡∏µ‡∏ß‡∏¥‡∏ï‡∏≠‡∏¢‡∏π‡πà
				if IsPlayerAlive(player) then
					local head = player.Character.Head
					local headPos, onScreen = Camera:WorldToViewportPoint(head.Position)
					if onScreen then
						local head2D = Vector2.new(headPos.X, headPos.Y)
						local distance = (head2D - screenCenter).Magnitude
						
						-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö Wall Check ‡∏î‡πâ‡∏ß‡∏¢
						if distance <= radius and distance < shortestDistance and CanSeeTarget(head) then
							shortestDistance = distance
							closestPlayer = player
						end
					end
				end
			end
		end
	end

	return closestPlayer
end

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏£‡∏±‡∏ö‡∏Ç‡∏ô‡∏≤‡∏î‡∏ß‡∏á‡∏Å‡∏•‡∏°
local function SetCircleSize(sizePx)
	radius = sizePx / 2
	circle.Size = UDim2.fromOffset(sizePx, sizePx)
end

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î AimLock
local function ToggleAimLock(state)
	lockEnabled = state
	circle.Visible = state
	if not state then
		currentTarget = nil
		UpdateTargetInfo(nil)
		if currentTween then
			currentTween:Cancel()
			currentTween = nil
		end
	end
end

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Team Check
local function ToggleTeamCheck(state)
	teamCheckEnabled = state
end

--// ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏•‡πá‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏ó‡∏∏‡∏Å‡πÄ‡∏ü‡∏£‡∏° (‡∏à‡∏≥‡∏Å‡∏±‡∏î‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏à‡∏£‡∏¥‡∏á‡∏ï‡∏≤‡∏° updateInterval)
RunService.RenderStepped:Connect(function(deltaTime)
	if not lockEnabled then return end

	accumulatedTime += deltaTime
	if accumulatedTime >= updateInterval then
		accumulatedTime = 0
		local target = GetClosestTarget()
		if target and target.Character and target.Character:FindFirstChild("Head") then
			currentTarget = target
			LockToHead(target.Character.Head)
			UpdateTargetInfo(target)
		else
			if currentTarget then
				currentTarget = nil
				UpdateTargetInfo(nil)
			end
		end
	end
end)

--// UI Framework: PlayerTab toggle ‡πÅ‡∏•‡∏∞ slider (‡∏™‡∏°‡∏°‡∏ï‡∏¥ PlayerTab ‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß)
PlayerTab:Toggle({
	Title = "‡∏•‡πá‡∏≠‡∏Å‡∏´‡∏±‡∏ß",
	Value = false,
	Callback = function(value)
		ToggleAimLock(value)
	end
})

PlayerTab:Slider({
	Title = "‡∏Ç‡∏ô‡∏≤‡∏î‡∏£‡∏∞‡∏¢‡∏∞‡∏•‡πá‡∏≠‡∏Å",
	Min = 50,
	Max = 400,
	Value = radius * 2,
	Callback = function(val)
		SetCircleSize(val)
	end
})

PlayerTab:Toggle({
	Title = "‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ó‡∏µ‡∏° (‡πÑ‡∏°‡πà‡∏•‡πá‡∏≠‡∏Å‡∏ó‡∏µ‡∏°‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô)",
	Value = teamCheckEnabled,
	Callback = function(value)
		ToggleTeamCheck(value)
	end
})
